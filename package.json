{
  "name": "mui-app-container",
  "version": "0.3.0",
  "description": "Orchestrates Material-UI's AppBar and Drawer components based on device width",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "repository": "https://github.com/techniq/mui-app-container",
  "author": "Sean Lynch <techniq35@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "material-ui": "^1.0.0-beta.43",
    "react": "^16.3.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.6.1"
  },
  "devDependencies": {
    "@storybook/react": "^3.4.2",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "gh-pages": "^1.1.0",
    "material-ui": "^1.0.0-beta.43",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "scripts": {
    "build": "NODE_ENV=production babel src -d dist",
    "storybook": "start-storybook -p 9009 -c stories",
